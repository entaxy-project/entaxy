{
    "name": "entaxy",
    "description": "Your Personal Finance App",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://entaxy.io",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "coverage": "react-scripts test --coverage --watchAll=false",
        "test:ci": "react-scripts test --coverage --watchAll=false",
        "eject": "react-scripts eject",
        "lint": "eslint src --ext '.js,.jsx' --config .eslintrc",
        "lint:fix": "npm run lint --fix",
        "deploy:staging": "npm run build && aws s3 sync build/ s3://entaxy-staging",
        "deploy:production": "npm run build && aws s3 sync build/ s3://entaxy-production",
        "deploy:test": "npm run build && aws s3 sync build/ s3://entaxy-test"
    },
    "dependencies": {
        "@material-ui/core": "^4.2.1",
        "@material-ui/icons": "^4.2.1",
        "@mdi/js": "^3.8.95",
        "@mdi/react": "^1.2.1",
        "@vx/axis": "0.0.190",
        "@vx/curve": "0.0.189",
        "@vx/glyph": "0.0.190",
        "@vx/gradient": "0.0.189",
        "@vx/grid": "0.0.189",
        "@vx/group": "0.0.190",
        "@vx/legend": "0.0.190",
        "@vx/pattern": "0.0.189",
        "@vx/point": "0.0.189",
        "@vx/responsive": "0.0.189",
        "@vx/scale": "0.0.190",
        "@vx/shape": "0.0.190",
        "@vx/tooltip": "0.0.189",
        "big.js": "^5.2.2",
        "blockstack": "^19.2.1",
        "chroma-js": "^2.0.4",
        "classnames": "^2.2.6",
        "d3-array": "^2.2.0",
        "date-fns": "^1.30.1",
        "formik": "^1.5.8",
        "jsdom": "^15.1.1",
        "lodash": "^4.17.15",
        "papaparse": "^5.0.0",
        "pluralize": "^8.0.0",
        "prop-types": "^15.7.2",
        "query-string": "^6.8.0",
        "react": "^16.8.6",
        "react-confirm": "^0.1.18",
        "react-dom": "^16.8.6",
        "react-dropzone": "^10.1.5",
        "react-motion": "^0.5.2",
        "react-number-format": "^4.0.8",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "^3.0.1",
        "react-select": "^3.0.4",
        "react-virtualized": "^9.21.1",
        "react-window": "^1.8.5",
        "recharts": "^1.6.2",
        "recompose": "^0.30.0",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "uuid": "^3.3.2",
        "yup": "^0.27.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "coveralls": "^3.0.5",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-plugin-jest": "^22.12.0",
        "husky": "^3.0.1",
        "jest-enzyme": "^7.0.2",
        "lint-staged": "^9.2.0",
        "react-test-renderer": "^16.8.6",
        "redux-mock-store": "^1.5.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "npm run lint --fix",
            "react-scripts test --env=jsdom --bail --coverage --findRelatedTests --watchAll=false",
            "git add"
        ]
    }
}