{
    "name": "entaxy",
    "description": "Your Personal Finance App",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://entaxy.io",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --coverage --watch --onlyChanged",
        "eject": "react-scripts eject",
        "lint": "eslint src --ext '.js,.jsx' --config .eslintrc",
        "lint:fix": "npm run lint --fix",
        "deploy:staging": "npm run build && aws s3 sync build/ s3://entaxy-staging",
        "deploy:test": "npm run build && aws s3 sync build/ s3://entaxy-test"
    },
    "dependencies": {
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "@mdi/js": "^3.5.95",
        "@mdi/react": "^1.1.0",
        "@vx/axis": "0.0.184",
        "@vx/curve": "0.0.182",
        "@vx/group": "0.0.183",
        "@vx/pattern": "0.0.183",
        "@vx/point": "0.0.182",
        "@vx/responsive": "0.0.186",
        "@vx/scale": "0.0.182",
        "@vx/shape": "0.0.184",
        "@vx/tooltip": "0.0.184",
        "big.js": "^5.2.2",
        "blockstack": "^18.3.0",
        "classnames": "^2.2.6",
        "d3-array": "^2.0.3",
        "date-fns": "^1.30.1",
        "formik": "^1.5.1",
        "jsdom": "^13.2.0",
        "lodash": "^4.17.11",
        "papaparse": "^4.6.3",
        "prop-types": "^15.7.2",
        "query-string": "^6.3.0",
        "react": "^16.8.3",
        "react-confirm": "^0.1.18",
        "react-dom": "^16.8.3",
        "react-dropzone": "^9.0.0",
        "react-motion": "^0.5.2",
        "react-number-format": "^4.0.6",
        "react-redux": "^6.0.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.1.5",
        "react-select": "^2.4.1",
        "react-virtualized": "^9.21.0",
        "recompose": "^0.30.0",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.8",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "uuid": "^3.3.2",
        "yup": "^0.26.10"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "coveralls": "^3.0.3",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-jest": "^22.3.0",
        "husky": "^1.3.1",
        "jest-enzyme": "^7.0.1",
        "lint-staged": "^8.1.5",
        "react-test-renderer": "^16.8.3",
        "redux-mock-store": "^1.5.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "npm run lint --fix",
            "react-scripts test --env=jsdom --bail --coverage --findRelatedTests",
            "git add"
        ]
    }
}
